{
	"compilerOptions": {
		"module": "commonjs",
		"target": "esnext",
		"lib": [
			"es2015",
			"es2016",
			"es2017",
			"es2018",
			"esnext",
			"es5",
			"es6",
			"es7"
		],
		"declaration": false,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"noImplicitAny": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"outDir": "./out",
		"removeComments": true,
		"sourceMap": true,
		"strict": true,
		"strictNullChecks": true,
		"strictPropertyInitialization": true,
		"types": [
			"node",
		],
		"moduleResolution": "node",
		"noEmitOnError": true
	},
	"include": [
		"ecosystem.config.ts"
	]
}
